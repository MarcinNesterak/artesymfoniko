# Port dla serwera backend
PORT=3002

# MongoDB Atlas connection string
# Zamień na swój prawdziwy connection string z MongoDB Atlas
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/orchestra?retryWrites=true&w=majority

# JWT Secret - użyj długiego, bezpiecznego klucza
JWT_SECRET=your_super_secret_jwt_key_here_make_it_very_long_and_secure

# Environment
NODE_ENV=development

# CORS - dozwolone domeny frontend
FRONTEND_URL=http://localhost:3000